<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu_for_employees"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.victo.salarymanagement.Activities.MainMenuForEmployees">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Register timesheet"
        android:id="@+id/btnRegisterTimeSheet"
        android:onClick="registerTimeSheet"/>

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/btnSeeHistory"
        android:text="See history"
        android:onClick="seeHistory"/>




</LinearLayout>
